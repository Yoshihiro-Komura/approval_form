var TR = new GlideRecord('x_136731_approval_approval_choise_data');&#13;
TR.query();&#13;
while(TR.next()) {&#13;
	if(TR.getValue('u_approval_title') == current.u_select) {&#13;
		if(TR.getDisplayValue('u_approval_counter_date') != DT) {&#13;
			TR.setValue('u_approval_counter_date', DT);&#13;
			TR.setValue('u_approval_counter', 0);&#13;
		}&#13;
		var CC = TR.getValue('u_approval_counter');&#13;
		CC++;&#13;
		TR.setValue('u_approval_counter', CC);&#13;
		TR.update();&#13;
		var WORD = TR.getValue('u_approval_title_initial') + DT.substr(0, 4);&#13;
		WORD += DT.substr(5, 2) + DT.substr(8, 2);&#13;
		WORD += ('000' + CC).substr(-4);&#13;
		current.u_counter = WORD;&#13;
	}&#13;
}</value>
